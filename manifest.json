{
  "manifest_version": 2,
  "name": "Web Acknowledgement",
  "description": "A Chrome extension that performs a land acknowledgement based on where the website is stored physically",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*",
        "file://*"
      ],
      "js": [
        "private_keys.js",
        "jquery-3.5.1.min.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {

  },
  "permissions": [
    "tabs"
  ]
}
